{
  "expo": {
    "name": "connect-circle-rn",
    "slug": "connect-circle-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/app-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f1efe7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.khervie00.connectcirclern"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-app-icon.png",
        "backgroundColor": "#f1efe7"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8c054963-86c2-4020-a52d-a11cccdd801c"
      }
    }
  }
}
